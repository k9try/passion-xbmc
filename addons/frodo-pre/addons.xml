<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="repository.passion.xbmc.frodo.pre" name="Passion-XBMC.org Add-ons Frodo-pre"
       version="3.0.0" provider-name="Team Passion-XBMC">
    <extension point="xbmc.addon.repository"
        name="Google Passion-XBMC Add-on Repository">
        <info compressed="false">http://passion-xbmc.googlecode.com/svn/trunk/addons/frodo-pre/addons.xml</info>
        <checksum>http://passion-xbmc.googlecode.com/svn/trunk/addons/frodo-pre/addons.xml.md5</checksum>
        <datadir zip="false">http://passion-xbmc.googlecode.com/svn/trunk/addons/frodo-pre</datadir>
        <hashes>true</hashes>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary lang="en">Install Add-ons from passion-xbmc.org for XBMC Frodo-pre</summary>
        <summary lang="fr">Installez des Add-ons depuis passion-xbmc.org for XBMC Frodo-pre</summary>
        <description lang="en">Download and install Add-ons from passion-xbmc.org pour XBMC Frodo-pre</description>
        <description lang="fr">Téléchargez et installez des Add-ons depuis passion-xbmc.org pour XBMC Frodo-pre</description>
        <platform>all</platform>
    </extension>
</addon>

<addon id="script.module.xbmcart"
       name="XBMC Artwork"
       version="1.0.0"
       provider-name="Frost">
  <requires>
    <import addon="xbmc.python" version="2.0"/>
  </requires>
  <extension point="xbmc.python.module"
             library="lib" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en"></summary>
    <description lang="en"></description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="script.moviesets" name="Movie Sets (frodo-pre)"
       version="3.0.0" provider-name="Frost (passion-xbmc.org)">
  <requires>
    <import addon="xbmc.python" version="2.0"/>
    <import addon="script.module.pil" version="1.1.7" optional="true" />
  </requires>
  <extension point="xbmc.python.script" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Movie Sets Library and Manager</summary>
    <description lang="en">Movie Sets Library for skins / skinners and Manager for users</description>
    <disclaimer lang="en">Comparison of skin features.[CR]http://wiki.xbmc.org/index.php?title=Comparison_of_skin_features</disclaimer>
    <license>GPL</license>
    <platform>all</platform>
  </extension>
</addon>
</addons>
