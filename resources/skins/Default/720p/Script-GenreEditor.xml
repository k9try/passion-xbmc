<window type="dialog" id="25622">
	<zorder>1</zorder> 
	<defaultcontrol always="true">120</defaultcontrol>
	<coordinates>
		<system>1</system>
		<posx>450</posx>
		<posy>35</posy>
	</coordinates>
  
	<controls>
