				<height>555</height>
				<onleft>22</onleft>
				<onright>22</onright>
				<onup>120</onup>
				<ondown>120</ondown>
				<viewtype label="535">list</viewtype>
				<pagecontrol>121</pagecontrol>
				<scrolltime>200</scrolltime>
				<itemlayout width="350" height="17">
