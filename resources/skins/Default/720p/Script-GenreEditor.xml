						<posx>0</posx>
						<posy>0</posy>
						<width>17</width>
						<height>17</height>
						<info>Listitem.Icon</info>
					</control>
					<control type="label">
						<posx>20</posx>
						<posy>0</posy>
						<width>320</width>
						<height>17</height>
